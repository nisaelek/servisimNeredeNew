<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    tools:context=".AnaActivity">


    <androidx.appcompat.widget.Toolbar
        android:id="@+id/toolbar3"
        android:layout_width="439dp"
        android:layout_height="66dp"
        android:background="@color/a"
        android:minHeight="?attr/actionBarSize"
        android:theme="?attr/actionBarTheme"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/txtKullanici"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="32dp"
        android:layout_marginTop="32dp"
        android:layout_marginEnd="32dp"
        android:text="TextView"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.49"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/toolbar3" />

    <TextView
        android:id="@+id/textView0"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Engelsiz Servis"
        android:textColor="@color/white"
        android:textSize="24sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/toolbar3"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.633"
        app:layout_constraintStart_toStartOf="@+id/toolbar3"
        app:layout_constraintTop_toTopOf="@+id/toolbar3" />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="107dp"
        android:layout_height="65dp"
        android:layout_marginEnd="48dp"
        app:layout_constraintBottom_toBottomOf="@+id/toolbar3"
        app:layout_constraintEnd_toStartOf="@+id/textView0"
        app:layout_constraintHorizontal_bias="0.052"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/toolbar3"
        app:srcCompat="@mipmap/ic_launcher" />

    <Button
        android:id="@+id/takiplbtn"
        android:layout_width="113dp"
        android:layout_height="73dp"
        android:layout_marginTop="20dp"
        android:background="@drawable/ic_location"
        android:onClick="takiplgit"
        android:shadowColor="@color/green"
        app:layout_constraintEnd_toEndOf="@+id/txtKullanici"
        app:layout_constraintStart_toStartOf="@+id/txtKullanici"
        app:layout_constraintTop_toBottomOf="@+id/txtKullanici" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="80dp"
        android:layout_height="39dp"
        android:layout_marginTop="16dp"
        android:text="Takip Listesi"
        android:textColor="@color/iconcolor"
        app:layout_constraintEnd_toEndOf="@+id/takiplbtn"
        app:layout_constraintHorizontal_bias="0.121"
        app:layout_constraintStart_toStartOf="@+id/takiplbtn"
        app:layout_constraintTop_toBottomOf="@+id/takiplbtn" />

    <Button
        android:id="@+id/uploadbtn"
        android:layout_width="113dp"
        android:layout_height="73dp"
        android:layout_marginTop="80dp"
        android:background="@drawable/ic_add"
        android:onClick="uploadaGit"
        android:shadowColor="@color/green"
        app:layout_constraintEnd_toEndOf="@+id/eklebtn"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="@+id/eklebtn"
        app:layout_constraintTop_toBottomOf="@+id/eklebtn" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="101dp"
        android:layout_height="50dp"
        android:layout_marginTop="16dp"
        android:text="Servis Fotoğraflarını Yükle"
        android:textColor="@color/iconcolor"
        app:layout_constraintEnd_toEndOf="@+id/uploadbtn"
        app:layout_constraintHorizontal_bias="0.321"
        app:layout_constraintStart_toStartOf="@+id/uploadbtn"
        app:layout_constraintTop_toBottomOf="@+id/uploadbtn" />

    <Button
        android:id="@+id/eklebtn"
        android:layout_width="113dp"
        android:layout_height="73dp"
        android:layout_marginTop="68dp"
        android:background="@drawable/ic_photo"
        android:onClick="shareGit"
        android:shadowColor="@color/green"
        app:layout_constraintEnd_toEndOf="@+id/takiplbtn"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/takiplbtn"
        app:layout_constraintTop_toBottomOf="@+id/takiplbtn"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/textView1"
        android:layout_width="115dp"
        android:layout_height="38dp"
        android:layout_marginTop="8dp"
        android:text="Yüklenen Fotoğraflar"
        android:textColor="@color/iconcolor"
        app:layout_constraintEnd_toEndOf="@+id/eklebtn"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="@+id/eklebtn"
        app:layout_constraintTop_toBottomOf="@+id/eklebtn" />

    <Button
        android:id="@+id/sharebtn"
        android:layout_width="113dp"
        android:layout_height="73dp"
        android:layout_marginTop="64dp"
        android:background="@drawable/ic_profile"
        android:onClick="profileGit"
        android:shadowColor="@color/green"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/uploadbtn"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/uploadbtn"
        app:layout_constraintTop_toBottomOf="@+id/uploadbtn"
        app:layout_constraintVertical_bias="0.421"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="75dp"
        android:layout_height="26dp"
        android:text="Profilim"
        android:textColor="@color/iconcolor"
        app:layout_constraintEnd_toEndOf="@+id/sharebtn"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/sharebtn"
        app:layout_constraintTop_toBottomOf="@+id/sharebtn" />
</androidx.constraintlayout.widget.ConstraintLayout>