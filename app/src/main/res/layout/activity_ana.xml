<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    tools:context=".AnaActivity">


    <TextView
        android:id="@+id/textView12"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginBottom="28dp"
        android:text="                           Mesajlarınızı kontrol etmek için tıklayınız."
        android:textAlignment="gravity"
        android:textColor="@color/iconcolor"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView3" />

    <Button
        android:id="@+id/ograra2"
        android:layout_width="0dp"
        android:layout_height="56dp"
        android:layout_marginTop="95dp"
        android:layout_marginEnd="57dp"
        android:layout_marginBottom="95dp"
        android:background="@drawable/ic_sara"
        android:onClick="aramaGit"
        android:shadowColor="@color/green"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/uploadbtn"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/takiplbtn"
        app:layout_constraintTop_toBottomOf="@+id/toolbar3"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/ograra"
        android:layout_width="0dp"
        android:layout_height="56dp"
        android:layout_marginTop="76dp"
        android:layout_marginEnd="55dp"
        android:layout_marginBottom="77dp"
        android:background="@drawable/ic_ogr_arama"
        android:onClick="shareGit"
        android:shadowColor="@color/white"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/textView7"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/sharebtn"
        app:layout_constraintTop_toTopOf="@+id/textView4"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="0dp"
        android:layout_height="68dp"
        android:layout_marginTop="56dp"
        android:layout_marginEnd="68dp"
        android:layout_marginBottom="57dp"
        android:text="Dilek ve Şikayet Kutusu"
        android:textColor="@color/iconcolor"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/chatbtn"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/textView2"
        app:layout_constraintTop_toTopOf="@+id/sharebtn" />

    <androidx.appcompat.widget.Toolbar
        android:id="@+id/toolbar3"
        android:layout_width="439dp"
        android:layout_height="0dp"
        android:layout_marginBottom="96dp"
        android:background="@color/a"
        android:minHeight="?attr/actionBarSize"
        android:theme="?attr/actionBarTheme"
        app:layout_constraintBottom_toTopOf="@+id/takiplbtn"
        app:layout_constraintStart_toStartOf="@+id/imageView3"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView0"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="15dp"
        android:layout_marginEnd="88dp"
        android:text="Engelsiz Servis"
        android:textColor="@color/white"
        android:textSize="24sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/imageView3"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginTop="6dp"
        android:layout_marginEnd="22dp"
        android:layout_marginBottom="625dp"
        app:layout_constraintBottom_toTopOf="@+id/textView12"
        app:layout_constraintEnd_toStartOf="@+id/textView0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@mipmap/ic_launcher" />

    <Button
        android:id="@+id/takiplbtn"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginStart="42dp"
        android:layout_marginEnd="88dp"
        android:layout_marginBottom="9dp"
        android:background="@drawable/ic_location"
        android:onClick="takiplgit"
        android:shadowColor="@color/green"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/textView5"
        app:layout_constraintEnd_toStartOf="@+id/ograra2"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/toolbar3" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginStart="45dp"
        android:layout_marginEnd="108dp"
        android:layout_marginBottom="30dp"
        android:text="Takip Listesi"
        android:textColor="@color/iconcolor"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/eklebtn"
        app:layout_constraintEnd_toStartOf="@+id/textView11"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/takiplbtn" />

    <Button
        android:id="@+id/uploadbtn"
        android:layout_width="0dp"
        android:layout_height="57dp"
        android:layout_marginTop="150dp"
        android:layout_marginEnd="61dp"
        android:layout_marginBottom="150dp"
        android:background="@drawable/ic_add"
        android:onClick="uploadaGit"
        android:shadowColor="@color/green"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/textView7"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/eklebtn"
        app:layout_constraintTop_toTopOf="@+id/takiplbtn"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="0dp"
        android:layout_height="50dp"
        android:layout_marginTop="216dp"
        android:layout_marginEnd="61dp"
        android:layout_marginBottom="216dp"
        android:text="Servis Fotoğraflarını Yükle"
        android:textColor="@color/iconcolor"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/chatbtn"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/textView1"
        app:layout_constraintTop_toTopOf="@+id/takiplbtn" />

    <Button
        android:id="@+id/eklebtn"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginStart="40dp"
        android:layout_marginEnd="84dp"
        android:layout_marginBottom="7dp"
        android:background="@drawable/ic_photo"
        android:onClick="shareGit"
        android:shadowColor="@color/green"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/textView1"
        app:layout_constraintEnd_toStartOf="@+id/uploadbtn"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView5"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/textView1"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginStart="45dp"
        android:layout_marginEnd="89dp"
        android:layout_marginBottom="35dp"
        android:text="Yüklenen Fotoğraflar"
        android:textColor="@color/iconcolor"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/sharebtn"
        app:layout_constraintEnd_toStartOf="@+id/textView4"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/eklebtn" />

    <Button
        android:id="@+id/sharebtn"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginStart="40dp"
        android:layout_marginEnd="88dp"
        android:layout_marginBottom="10dp"
        android:background="@drawable/ic_profile"
        android:onClick="profileGit"
        android:shadowColor="@color/green"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/textView2"
        app:layout_constraintEnd_toStartOf="@+id/ograra"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView1"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginStart="45dp"
        android:layout_marginEnd="108dp"
        android:layout_marginBottom="61dp"
        android:text="Profilim"
        android:textColor="@color/iconcolor"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/chatbtn"
        app:layout_constraintEnd_toStartOf="@+id/textView7"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/sharebtn" />

    <TextView
        android:id="@+id/textView11"
        android:layout_width="0dp"
        android:layout_height="56dp"
        android:layout_marginTop="9dp"
        android:layout_marginEnd="68dp"
        android:text="Öğrenci Veli  Şöför Arama"
        android:textColor="@color/iconcolor"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/textView5"
        app:layout_constraintTop_toBottomOf="@+id/ograra2"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/chatbtn"
        android:layout_width="78dp"
        android:layout_height="0dp"
        android:background="@drawable/ic_chat"
        android:onClick="chatbtn"
        android:shadowColor="@color/green"
        app:layout_constraintBottom_toTopOf="@+id/textView12"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView2" />


</androidx.constraintlayout.widget.ConstraintLayout>