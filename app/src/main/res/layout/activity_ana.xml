<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    tools:context=".AnaActivity">


    <TextView
        android:id="@+id/textView12"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="                           Mesajlarınızı kontrol etmek için tıklayınız."
        android:textColor="@color/iconcolor"
        app:layout_constraintTop_toBottomOf="@+id/chat_odasi"
        tools:layout_editor_absoluteX="60dp" />

    <ImageView
        android:id="@+id/chat_odasi"
        android:layout_width="452dp"
        android:layout_height="51dp"
        app:srcCompat="@drawable/ic_chat"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="703dp" />

    <Button
        android:id="@+id/ograra2"
        android:layout_width="113dp"
        android:layout_height="73dp"
        android:layout_marginEnd="92dp"
        android:background="@drawable/ic_sara"
        android:onClick="shareGit"
        android:shadowColor="@color/green"
        app:layout_constraintEnd_toStartOf="@+id/ograra"
        app:layout_constraintHorizontal_bias="0.932"
        app:layout_constraintStart_toStartOf="parent"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteY="521dp" />

    <Button
        android:id="@+id/ograra"
        android:layout_width="113dp"
        android:layout_height="73dp"
        android:layout_marginEnd="60dp"
        android:background="@drawable/ic_ogr_arama"
        android:onClick="shareGit"
        android:shadowColor="@color/green"
        app:layout_constraintEnd_toEndOf="parent"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteY="521dp" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="115dp"
        android:layout_height="38dp"
        android:text="Dilek ve Şikayet Kutusu"
        android:textColor="@color/iconcolor"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.658"
        app:layout_constraintStart_toEndOf="@+id/textView11"
        app:layout_constraintTop_toBottomOf="@+id/ograra"
        app:layout_constraintVertical_bias="0.233" />

    <androidx.appcompat.widget.Toolbar
        android:id="@+id/toolbar3"
        android:layout_width="439dp"
        android:layout_height="66dp"
        android:background="@color/a"
        android:minHeight="?attr/actionBarSize"
        android:theme="?attr/actionBarTheme"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView0"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Engelsiz Servis"
        android:textColor="@color/white"
        android:textSize="24sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/toolbar3"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.633"
        app:layout_constraintStart_toStartOf="@+id/toolbar3"
        app:layout_constraintTop_toTopOf="@+id/toolbar3" />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="107dp"
        android:layout_height="65dp"
        android:layout_marginEnd="48dp"
        app:layout_constraintBottom_toBottomOf="@+id/toolbar3"
        app:layout_constraintEnd_toStartOf="@+id/textView0"
        app:layout_constraintHorizontal_bias="0.052"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/toolbar3"
        app:srcCompat="@mipmap/ic_launcher" />

    <Button
        android:id="@+id/takiplbtn"
        android:layout_width="113dp"
        android:layout_height="73dp"
        android:layout_marginTop="64dp"
        android:background="@drawable/ic_location"
        android:onClick="takiplgit"
        android:shadowColor="@color/green"
        app:layout_constraintEnd_toEndOf="@+id/txtKullanici"
        app:layout_constraintHorizontal_bias="0.092"
        app:layout_constraintStart_toStartOf="@+id/txtKullanici"
        app:layout_constraintTop_toBottomOf="@+id/txtKullanici" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="80dp"
        android:layout_height="39dp"
        android:layout_marginTop="20dp"
        android:text="Takip Listesi"
        android:textColor="@color/iconcolor"
        app:layout_constraintEnd_toEndOf="@+id/takiplbtn"
        app:layout_constraintHorizontal_bias="0.484"
        app:layout_constraintStart_toStartOf="@+id/takiplbtn"
        app:layout_constraintTop_toBottomOf="@+id/takiplbtn" />

    <TextView
        android:id="@+id/txtKullanici"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="32dp"
        android:layout_marginTop="12dp"
        android:layout_marginEnd="32dp"
        android:text="TextView"
        android:textColor="#FFFFFF"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.111"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/toolbar3" />

    <Button
        android:id="@+id/uploadbtn"
        android:layout_width="113dp"
        android:layout_height="73dp"
        android:layout_marginTop="32dp"
        android:background="@drawable/ic_add"
        android:onClick="uploadaGit"
        android:shadowColor="@color/green"
        app:layout_constraintTop_toBottomOf="@+id/textView5"
        tools:layout_editor_absoluteX="60dp"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="101dp"
        android:layout_height="50dp"
        android:layout_marginTop="16dp"
        android:text="Servis Fotoğraflarını Yükle"
        android:textColor="@color/iconcolor"
        app:layout_constraintEnd_toEndOf="@+id/uploadbtn"
        app:layout_constraintHorizontal_bias="0.321"
        app:layout_constraintStart_toStartOf="@+id/uploadbtn"
        app:layout_constraintTop_toBottomOf="@+id/uploadbtn" />

    <Button
        android:id="@+id/eklebtn"
        android:layout_width="113dp"
        android:layout_height="73dp"
        android:background="@drawable/ic_photo"
        android:onClick="shareGit"
        android:shadowColor="@color/green"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.629"
        app:layout_constraintStart_toEndOf="@+id/takiplbtn"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteY="161dp" />

    <TextView
        android:id="@+id/textView1"
        android:layout_width="115dp"
        android:layout_height="38dp"
        android:layout_marginTop="4dp"
        android:text="Yüklenen Fotoğraflar"
        android:textColor="@color/iconcolor"
        app:layout_constraintEnd_toEndOf="@+id/eklebtn"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/eklebtn"
        app:layout_constraintTop_toBottomOf="@+id/eklebtn" />

    <Button
        android:id="@+id/sharebtn"
        android:layout_width="113dp"
        android:layout_height="73dp"
        android:layout_marginTop="50dp"
        android:background="@drawable/ic_profile"
        android:onClick="profileGit"
        android:shadowColor="@color/green"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.65"
        app:layout_constraintStart_toEndOf="@+id/uploadbtn"
        app:layout_constraintTop_toBottomOf="@+id/textView1"
        app:layout_constraintVertical_bias="0.0"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="75dp"
        android:layout_height="26dp"
        android:layout_marginTop="24dp"
        android:text="Profilim"
        android:textColor="@color/iconcolor"
        app:layout_constraintEnd_toEndOf="@+id/sharebtn"
        app:layout_constraintHorizontal_bias="0.447"
        app:layout_constraintStart_toStartOf="@+id/sharebtn"
        app:layout_constraintTop_toBottomOf="@+id/sharebtn" />

    <TextView
        android:id="@+id/textView11"
        android:layout_width="99dp"
        android:layout_height="48dp"
        android:text="Öğrenci Veli  Şöför Arama"
        android:textColor="@color/iconcolor"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/ograra2"
        app:layout_constraintVertical_bias="0.247"
        tools:layout_editor_absoluteX="74dp"
        tools:ignore="MissingConstraints" />

</androidx.constraintlayout.widget.ConstraintLayout>